//lag ny "<p></p>"
const para = document.createElement("p");


//skriv inni "<p></p>"
const node = document.createTextNode("This is new.")
//koble node med para
para.appendChild(node);


console.log(para);


//legg para i body på html
document.body.appendChild(para);



const artHead = document.createElement("h3");
const artBody = document.createElement("p");

const title = document.createTextNode("Cool Article About Cats");
const body = document.createTextNode("cats are assholes.");

const container = document.querySelector("#generated");

artHead.appendChild(title);
artBody.appendChild(body);

container.appendChild(artHead);
container.appendChild(artBody);








